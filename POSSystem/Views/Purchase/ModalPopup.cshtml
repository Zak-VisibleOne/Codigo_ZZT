<div class="modal fade bs-example-modal-lg" id="LineDetailModel" tabindex="-1" role="dialog" aria-labelledby="LineDetailModelLabel" aria-hidden="true">
    <div class="modal-dialog modal-lg">
        <disv class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title mt-0" id="LineDetailModelLabel">Detail</h5>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body">
                <div class="card-body">
                    <div class="form-group row" style="display:none;">
                        <label for="txtCode" class="col-sm-3 col-form-label AlignRight">Stock Code</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtCode" />
                        </div>
                    </div>
                    <div class="form-group row" style="display:none;">
                        <label for="txtCodeDesc" class="col-sm-3 col-form-label AlignRight">Stock Description</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control" id="txtCodeDesc" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="txtQty" class="col-sm-3 col-form-label AlignRight">Qty</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control AlignRight" id="txtQty" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="txtSalePrice" class="col-sm-3 col-form-label AlignRight">Price</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control AlignRight" id="txtPurchasePrice" />
                        </div>
                    </div>
                    <div class="form-group row">
                        <label for="txtAmount" class="col-sm-3 col-form-label AlignRight">Amount</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control AlignRight" id="txtAmount" disabled />
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-primary waves-effect waves-light" onclick="saveLineDetail()"> Save Changes </button>
            </div>
        </disv>
    </div>
</div>
<div class="modal fade bs-example-modal-xl" id="StockListModel" tabindex="-1" role="dialog" aria-labelledby="StockListModelLabel" aria-hidden="true">
    <div class="modal-dialog modal-xl">
        <disv class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title mt-0" id="StockListModelLabel">Stock list</h5>
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
            </div>
            <div class="modal-body">
                <div class="card-body">
                    <div class="table-responsive">
                        <table id="dtStockList" class="table table-striped table-bordered dt-responsive nowrap" style="border-collapse: collapse; border-spacing: 0; width: 100%;">
                            <thead style="background-color: #8b9bab; color: #fff;">
                                <tr>
                                    <th style="text-align:center;">Stock Code</th>
                                    <th style="text-align:center;">Stock Name</th>
                                    <th style="text-align:center;">Sale Price</th>
                                    <th style="text-align:center;">Purchase Price</th>
                                    <th style="text-align:center;">Category</th>
                                    <th style="text-align:center;">Action</th>
                                </tr>
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </disv>
    </div>
</div>